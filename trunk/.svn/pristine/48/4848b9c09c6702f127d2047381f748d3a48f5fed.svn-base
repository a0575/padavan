            MENUITEM "Block While &Disconnected",   IDSETT_BLOCKINTERNET_DISCONN
            MENUITEM "Allow Traffic to &Local Networks", IDSETT_BLOCKINTERNET_ALLOWLOCAL
        END
        POPUP "&Service Mode"
        BEGIN
            MENUITEM "&Don't Use a Service",        IDSETT_SERVICE_OFF
