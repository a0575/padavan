        MENUITEM "Go to &Web Page",             IDSETT_WEB_PAGE
        MENUITEM "See &Open Source Licenses",   IDSETT_OPENSOURCE
        MENUITEM SEPARATOR
        MENUITEM "&About TunSafe",              IDSETT_ABOUT
    END
